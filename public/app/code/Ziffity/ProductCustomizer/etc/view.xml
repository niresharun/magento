<?xml version="1.0"?>
<view xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:Config/etc/view.xsd">
    <media>
        <images module="Magento_Catalog">
            <image id="customframe_layer_image" type="layer_img">
                <width>100</width>
                <height>100</height>
            </image>
        </images>
    </media>
    <media>
        <images module="Magento_Catalog">
            <image id="customframe_thumb_img" type="img_thumb">
                <width>135</width>
                <height>135</height>
            </image>
        </images>
    </media>
    <media>
        <images module="Magento_Catalog">
            <image id="img_popup_preview_1" type="img_popup_preview_first">
                <width>285</width>
                <height>285</height>
            </image>
        </images>
    </media>
    <media>
        <images module="Magento_Catalog">
            <image id="img_popup_preview_2" type="img_popup_preview_second">
                <width>144</width>
                <height>215</height>
            </image>
        </images>
    </media>
</view>
